{
    "key": "owncloud",
    "label": "ownCloud",
    "image": "craigwillis/owncloud:latest",
    "description": "A self-hosted file sync and share server",
    "access": "external",
    "display": "stack",
    "ports": [
        { "port": 80, "protocol": "http" }
    ],
    "volumeMounts": [
      { "name": "owncloud", "mountPath": "/var/www/owncloud" }
    ]
}
